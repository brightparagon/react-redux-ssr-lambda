{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-unused-vars": "off",
    "require-jsdoc": "off",
    "no-console": "off",
    "quotes": "off",
    "new-cap": "off",
    "arrow-parens": "off",
    "arrow-body-style": "off",
    "semi": "off",
    "no-underscore-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "react/jsx-one-expression-per-line": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": "off"
  }
}
